    "czg": "^1.9.4",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.26.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.10",
    "naive-ui": "^2.39.0",
    "postcss": "^8.4.45",
    "postcss-html": "^1.7.0",
