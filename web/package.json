    "qs": "^6.13.0",
    "screenfull": "^6.0.2",
    "sortablejs": "^1.15.3",
    "vue": "^3.5.5",
    "vue-cropper": "^1.1.1",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@iconify/json": "^2.2.247",
